<table data-test-final-data>
    <thead>
    <tr>
      <th>Name</th>
      <th>Campus ID</th>
      <th>Group</th>
    </tr>
  </thead>
  <tbody>
    {{#each finalData as |obj|}}
      <tr>
        <td>{{obj.user.fullName}}</td>
        <td>{{obj.user.campusId}}</td>
        <td>{{obj.group.title}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

<button {{action (perform save)}} disabled={{save.isRunning}} data-test-finalize-users-submit>
  Save to Ilios
</button>
